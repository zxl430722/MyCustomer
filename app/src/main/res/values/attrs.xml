<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--菜单所在位置-->
    <attr name="position" format="enum">
        <enum name="left_top" value="1001" />
        <enum name="right_top" value="1002" />
        <enum name="left_bottom" value="1003" />
        <enum name="right_bottom" value="1004" />
    </attr>
    <!--半径 -->
    <attr name="radius" format="dimension"/>
    <declare-styleable name="SateliteMenu">
        <attr name="position" />
        <attr name="radius" />
    </declare-styleable>
</resources>